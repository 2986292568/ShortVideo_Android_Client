        <item name="icon_small_height">@dimen/dp_35</item>
        <item name="icon_small_width">@dimen/dp_35</item>
        <item name="icon_normal_height">@dimen/dp_40</item>
        <item name="icon_normal_width">@dimen/dp_40</item>
        <item name="icon_large_height">@dimen/dp_50</item>
        <item name="icon_large_width">@dimen/dp_50</item>

