        <item name="xui_config_color_explain_text">@color/txt_explain_color</item>
        <item name="android:textColor">@color/white</item>
        <item name="xui_config_content_spacing_horizontal">@dimen/dp_20</item>
        <!-- 自定义的属性 -->
        <item name="under_line_color">@color/underline_color</item>
        <!-- 按钮前景色 -->
        <item name="btn_foreground_color">@color/xui_config_color_white</item>
        <item name="btn_background_color">#cc4400</item>
        <item name="xui_actionbar_color">@color/block_color</item>
        <item name="android:windowBackground">@android:color/black</item>
        <!-- 输入框高度 -->
        <item name="input_height">@dimen/dp_50</item>
        <!-- 卡片显示得数据,标题或者字符，卡片的背景色 -->
        <item name="card_large_txt">@dimen/dp_30</item>
        <item name="card_background">#FFA500</item>
        <!-- recycler列表里照片的高度-->
        <item name="recycler_item_icon_height">@dimen/dp_60</item>
        <item name="recycler_item_icon_width">@dimen/dp_60</item>
        <item name="container_item_height">@dimen/dp_80</item>
