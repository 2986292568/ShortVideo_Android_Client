        <item name="mine_drawable_height">@dimen/dp_23</item>
        <item name="mine_drawable_width">@dimen/dp_20</item>
        <item name="mine_user_container_height">@dimen/dp_20</item>
        <item name="mine_user_container_width">@dimen/dp_60</item>
        <item name="mine_user_container_txt">@dimen/dp_12</item>
        <item name="info_item_height">@dimen/dp_45</item>
