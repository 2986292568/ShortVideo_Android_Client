        <item name="info_item_account_width">@dimen/dp_110</item>
        <!-- 卡片小按钮的尺寸 -->
        <item name="super_button_small_radius">@dimen/dp_4</item>
        <item name="super_button_small_width">@dimen/dp_42</item>
        <item name="super_button_small_height">@dimen/dp_20</item>
        <item name="super_button_small_txtSize">@dimen/dp_9</item>
        <item name="super_button_small_padding">@dimen/dp_3</item>
